<h1>Coins</h1>
<%= link_to 'Purchase Orders', purchase_orders_path %>
<table>
  <tr>
    <th>Symbol</th>
    <th>Coin Name</th>
    <th>Price</th>
  </tr>

  <% @coins.each do |coin| %>
    <tr>
      <td><%= coin.symbol %></td>
      <td><%= coin.coinName %></td>
      <td><%= coin.price %></td>
    </tr>
  <% end %>
</table>
